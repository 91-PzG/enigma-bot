{
  "name": "enigma-bot",
  "version": "0.2.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:docker": "nest build && docker build -f ./Dockerfile -t enigma-bot_main:latest .",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest/jest.config.ts"
  },
  "dependencies": {
    "@discord-nestjs/common": "^3.0.5",
    "@discord-nestjs/core": "^2.1.12",
    "@nestjs/axios": "^0.0.5",
    "@nestjs/common": "^8.4.3",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.4.3",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.4.3",
    "@nestjs/platform-socket.io": "^8.2.5",
    "@nestjs/schedule": "^1.1.0",
    "@nestjs/typeorm": "^8.0.3",
    "@nestjs/websockets": "^8.4.3",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "discord.js": "^13.3.1",
    "gamedig": "^3.0.8",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.0",
    "@nestjs/schematics": "^8.0.9",
    "@nestjs/testing": "^8.4.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/express-serve-static-core": "^4.17.28",
    "@types/gamedig": "^3.0.1",
    "@types/jest": "^27.4.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.7.9",
    "@types/passport": "^1.0.7",
    "@types/supertest": "^2.0.12",
    "@types/ws": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "jest": "^27.4.7",
    "jest-circus": "^27.4.6",
    "js-yaml": "^4.1.0",
    "jwt-decode": "^3.1.2",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.5.5"
  },
  "jest": {
    "collectCoverageFrom": [
      "**"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      ".dto.ts",
      ".decorator.ts",
      ".module.ts",
      ".entity.ts",
      ".enum.ts",
      ".config.ts",
      ".strategy.ts",
      "main.ts"
    ]
  }
}
